{
  "name": "wordpress-export-to-markdown",
  "version": "1.0.3",
  "description": "Converts a WordPress export XML file into Markdown files.",
  "main": "index.js",
  "repository": "https://github.com/lonekorean/wordpress-export-to-markdown.git",
  "keywords": [
    "wordpress",
    "markdown",
    "export"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js --postfolders=true --prefixdate=true --input ./export.xml --output output --yearmonthfolders=true --saveimages=true --addcontentimages=false"
  },
  "author": "Will Boyd <will@codersblock.com> (https://codersblock.com)",
  "license": "MIT",
  "engines": {
    "node": ">= 10.12.0"
  },
  "dependencies": {
    "luxon": "^1.21.3",
    "minimist": "^1.2.0",
    "request": "^2.88.0",
    "turndown": "^5.0.3",
    "xml2js": "^0.4.22"
  }
}
